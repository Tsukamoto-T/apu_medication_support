<?xml version="1.0"?>
<launch>
  <!---->
  <include file="$(find realsense2_camera)/launch/rs_camera.launch">
    <param name="/camera/realsense2_camera/filters" value="pointcloud"/>
    <arg name="filters"  value="pointcloud"/>
    <arg name="color_width" value="640"/>
    <arg name="color_height" value="480"/>
    <arg name="color_fps" value="30"/>
    <!--<arg name="initial_reset" value="true"/>
    <arg name="depth_width" value="640"/>
    <arg name="depth_height" value="480"/>
    <arg name="depth_fps" value="30"/>

    <arg name="align_depth" value="true"/>-->
  </include>
  <!---->
  <node name="apu_transparent_detection_node" pkg="apu_medication_support" type="transparent_detection" output="screen"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find apu_medication_support)/rviz/transparent.rviz" required="true" output="screen" />
</launch>
